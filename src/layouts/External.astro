---
import type { ExternalPost } from 'types';

type Props = ExternalPost;

const { external: href } = Astro.props.frontmatter;
---

<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="refresh" content={`0; URL=${href}`} />
  </head>
  <body>
    <p>
      If you are not redirected, <a href={href}>click here</a>.
    </p>
  </body>
</html>
