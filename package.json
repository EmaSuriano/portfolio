{
  "private": true,
  "name": "portfolio",
  "version": "1.0.0",
  "author": "Ema Suriano",
  "license": "MIT",
  "scripts": {
    "create-summary": "ts-node utils/create-summary.ts",
    "writer": "ts-node utils/writer.ts",
    "writer:watch": "ts-node utils/writer.ts --watch",
    "prebuild": "yarn writer",
    "build": "gatsby build",
    "build:netlify": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true yarn build --log-pages",
    "prestart": "yarn writer",
    "start": "gatsby develop",
    "png-to-jpg": "ls -1 ./content/**/*.png | xargs -n 1 bash -c 'convert \"$0\" \"${0%.png}.jpg\"'",
    "optimize-images": "imageoptim './content/**/*.jpg'",
    "lint": "eslint --cache --ext .ts,.tsx src",
    "check-types": "tsc -p . --noEmit"
  },
  "dependencies": {
    "@blocks/kit": "^0.0.28",
    "@narative/gatsby-theme-novela": "^0.18.7",
    "gatsby": "^2.32.13",
    "gatsby-plugin-google-analytics": "^2.11.0",
    "gatsby-plugin-manifest": "^2.12.1",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-typescript": "^2.12.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-typed": "^1.2.0"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "gatsby-paginate": "^1.1.1"
  },
  "devDependencies": {
    "@types/line-reader": "^0.0.34",
    "@types/react-dom": "^17.0.13",
    "@types/remove-markdown": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "image-size": "^1.0.1",
    "js-yaml": "^3.14.1",
    "link-check": "^5.0.2",
    "markdown-link-check": "^3.9.3",
    "prettier": "^2.6.2",
    "query-string": "^7.1.1",
    "remove-markdown": "^0.3.0",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint --fix"
    }
  },
  "resolutions": {
    "sharp": "0.25.4",
    "@babel/preset-env": "7.9.6"
  }
}
