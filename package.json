{
  "private": true,
  "name": "portfolio",
  "version": "1.0.0",
  "author": "Ema Suriano",
  "license": "MIT",
  "scripts": {
    "create-summary": "ts-node utils/create-summary.ts",
    "writer": "ts-node utils/writer.ts",
    "writer:watch": "ts-node utils/writer.ts --watch",
    "check-types": "tsc -p . --noEmit",
    "prebuild": "yarn writer",
    "build": "gatsby build",
    "build:netlify": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true yarn build --log-pages",
    "prestart": "yarn writer",
    "start": "gatsby develop",
    "check-markdown": "find ./content -name *.mdx -exec yarn markdown-link-check -c markdown-config.json {} \\;",
    "png-to-jpg": "ls -1 ./content/**/*.png | xargs -n 1 bash -c 'convert \"$0\" \"${0%.png}.jpg\"'",
    "optimize-images": "imageoptim './content/**/*.jpg'",
    "lint": "eslint --cache --ext .ts,.tsx src"
  },
  "dependencies": {
    "@blocks/kit": "^0.0.28",
    "@narative/gatsby-theme-novela": "^0.18.4",
    "gatsby": "^2.26.1",
    "gatsby-plugin-google-analytics": "^2.6.0",
    "gatsby-plugin-manifest": "^2.7.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-typescript": "^2.7.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-typed": "^1.2.0"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "gatsby-paginate": "^1.1.1"
  },
  "devDependencies": {
    "remove-markdown": "^0.3.0",
    "@types/line-reader": "^0.0.30",
    "@types/react-dom": "^16.9.9",
    "@types/remove-markdown": "^0.1.1",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.10.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "image-size": "^0.9.3",
    "js-yaml": "^3.13.1",
    "link-check": "^4.5.2",
    "markdown-link-check": "^3.8.3",
    "prettier": "^2.2.0",
    "query-string": "^6.13.7",
    "start-server-and-test": "^1.11.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint --fix"
    }
  }
}
