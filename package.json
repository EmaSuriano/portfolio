{
  "private": true,
  "name": "portfolio",
  "version": "0.0.0",
  "author": "Ema Suriano",
  "license": "MIT",
  "scripts": {
    "create-summary": "ts-node utils/create-summary.ts",
    "writer": "ts-node utils/writer.ts",
    "writer:watch": "ts-node utils/writer.ts --watch",
    "prebuild": "yarn writer",
    "build": "yarn check-types && gatsby build",
    "prestart": "yarn writer",
    "start": "gatsby develop",
    "preview": "gatsby serve",
    "png-to-jpg": "ls -1 ./content/**/*.png | xargs -n 1 bash -c 'convert \"$0\" \"${0%.png}.jpg\"'",
    "optimize-images": "imageoptim './content/**/*.jpg'",
    "lint": "eslint --cache --ext .ts,.tsx src",
    "check-types": "tsc -p . --noEmit",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@blocks/kit": "^0.0.28",
    "@narative/gatsby-theme-novela": "^0.18.7",
    "gatsby": "^2.32.13",
    "gatsby-plugin-google-analytics": "^2.11.0",
    "gatsby-plugin-manifest": "^2.12.1",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-typescript": "^2.12.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-typed": "^1.2.0"
  },
  "devDependencies": {
    "@types/line-reader": "^0.0.34",
    "postinstall-postinstall": "^2.1.0",
    "patch-package": "^6.5.0",
    "@types/react-dom": "^17.0.13",
    "@types/remove-markdown": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "image-size": "^1.0.2",
    "js-yaml": "^4.1.0",
    "link-check": "^5.2.0",
    "markdown-link-check": "^3.10.3",
    "prettier": "^2.7.1",
    "query-string": "^7.1.1",
    "remove-markdown": "^0.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "resolutions": {
    "sharp": "0.25.4",
    "@babel/preset-env": "7.9.6"
  }
}
