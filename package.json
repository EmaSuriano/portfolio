{
  "private": true,
  "name": "portfolio",
  "version": "1.0.0",
  "author": "Ema Suriano",
  "license": "MIT",
  "scripts": {
    "check-types": "tsc -p . --noEmit",
    "build": "yarn transpile-posts && gatsby build",
    "build:incremental": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true yarn build --log-pages",
    "start": "yarn transpile-posts --watch & gatsby develop",
    "transpile-posts": "ts-node utils/transpile-posts.ts",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "clean-and-serve": "yarn clean && yarn build && yarn serve",
    "check-markdown": "find ./content -name *.mdx -exec yarn markdown-link-check -c markdown-config.json {} \\;",
    "e2e": "cypress open",
    "e2e:run": "cypress run",
    "e2e:ci": "start-server-and-test 8000 e2e:run",
    "lint": "eslint --cache --ext .ts,.tsx src"
  },
  "dependencies": {
    "@blocks/kit": "^0.0.28",
    "@narative/gatsby-theme-novela": "^0.16.4",
    "gatsby": "^2.23.11",
    "gatsby-plugin-google-analytics": "^2.3.6",
    "gatsby-plugin-manifest": "^2.4.14",
    "gatsby-plugin-offline": "^3.2.13",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-typescript": "^2.4.13",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-typed": "^1.2.0"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "gatsby-paginate": "^1.1.1"
  },
  "devDependencies": {
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "cypress": "^4.9.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.1",
    "image-size": "^0.8.3",
    "js-yaml": "^3.13.1",
    "link-check": "^4.5.0",
    "markdown-link-check": "^3.8.1",
    "prettier": "^2.0.5",
    "start-server-and-test": "^1.11.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint --fix"
    }
  }
}
