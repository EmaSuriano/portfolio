{
  "private": true,
  "name": "portfolio",
  "version": "1.0.0",
  "author": "Ema Suriano",
  "license": "MIT",
  "scripts": {
    "create-summary": "ts-node utils/create-summary.ts",
    "transpile-posts": "ts-node utils/transpile-posts.ts",
    "update-readme": "ts-node utils/update-readme.ts",
    "check-types": "tsc -p . --noEmit",
    "prebuild": "yarn create-summary && yarn transpile-posts",
    "build": "gatsby build",
    "build:netlify": "yarn update-readme && GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true yarn build --log-pages",
    "prestart": "yarn create-summary && yarn transpile-posts",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "clean-and-serve": "yarn clean && yarn build && yarn serve",
    "check-markdown": "find ./content -name *.mdx -exec yarn markdown-link-check -c markdown-config.json {} \\;",
    "png-to-jpg": "ls -1 ./content/**/*.png | xargs -n 1 bash -c 'convert \"$0\" \"${0%.png}.jpg\"'",
    "optimize-images": "imageoptim './content/**/*.jpg'",
    "e2e": "cypress open",
    "e2e:run": "cypress run",
    "e2e:ci": "start-server-and-test 8000 e2e:run",
    "lint": "eslint --cache --ext .ts,.tsx src"
  },
  "dependencies": {
    "@blocks/kit": "^0.0.28",
    "@narative/gatsby-theme-novela": "^0.18.0",
    "axios": "^0.20.0",
    "gatsby": "^2.24.80",
    "gatsby-plugin-google-analytics": "^2.3.19",
    "gatsby-plugin-manifest": "^2.4.35",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-typescript": "^2.4.22",
    "react": "^16.14.0",
    "react-dom": "^16.13.1",
    "react-typed": "^1.2.0",
    "remove-markdown": "^0.3.0"
  },
  "peerDependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "gatsby-paginate": "^1.1.1"
  },
  "devDependencies": {
    "@types/line-reader": "^0.0.30",
    "@types/react-dom": "^16.9.8",
    "@types/remove-markdown": "^0.1.1",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "cypress": "^4.12.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "image-size": "^0.8.3",
    "js-yaml": "^3.13.1",
    "link-check": "^4.5.2",
    "markdown-link-check": "^3.8.3",
    "prettier": "^2.1.2",
    "query-string": "^6.13.6",
    "start-server-and-test": "^1.11.5",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint --fix"
    }
  }
}
